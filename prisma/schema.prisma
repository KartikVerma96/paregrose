generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  fullName  String
  email     String   @unique
  password  String?
  provider  String?  @default("credentials")
  providerId String?
  createdAt DateTime @default(now())
  updatedAt DateTime? @updatedAt
}